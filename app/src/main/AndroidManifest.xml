<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.menanga.ezparking">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".client.CMainActivity" />
        <activity android:name=".client.OutActivity" />
        <activity android:name=".client.InActivity" />
        <activity android:name=".client.RekapActivity" />
        <activity android:name=".client.CodeResultActivity"></activity>
    </application>

</manifest>